{
	"name": "express-boilerplate",
	"version": "0.0.1",
	"description": "",
	"main": "src/server.js",
	"scripts": {
		"test": "mocha --require test/setup.js",
		"dev": "nodemon src/server.js",
		"migrate": "postgrator --config postgrator-config.js",
		"migrate:test": "env NODE_ENV=test npm run migrate",
		"migrate:production": "env SSL=true DATABASE_URL=$(heroku config:get DATABASE_URL) npm run migrate",
		"start": "node src/server.js",
		"predeploy": "npm audit && npm run migrate:production && npm run seed:production",
		"deploy": "git push heroku master",
		"seed:production": "psql -U dunder_mifflin -d postgresql-transparent-37915 -f ./seeds/seed.tables.sql"
	},
	"keywords": [],
	"author": "Tom `tomatao` HT <tomht@thinkful.com>",
	"license": "MIT",
	"dependencies": {
		"bcryptjs": "^2.4.3",
		"cors": "^2.8.5",
		"dotenv": "^6.2.0",
		"express": "^4.16.4",
		"helmet": "^4.1.0",
		"jsonwebtoken": "^8.5.0",
		"knex": "^0.21.5",
		"morgan": "^1.9.1",
		"pg": "^7.18.2",
		"postgrator-cli": "^3.2.0",
		"react-icons": "^3.11.0",
		"xss": "^1.0.8"
	},
	"devDependencies": {
		"chai": "^4.2.0",
		"mocha": "^8.1.3",
		"nodemon": "^2.0.4",
		"supertest": "^3.3.0"
	}
}
